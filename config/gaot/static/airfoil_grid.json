{
    "setup": {
        "seed": 42,
        "device": "cuda:0",
        "dtype": "torch.float32",
        "trainer_name": "static",
        "train": true,
        "test": false,
        "ckpt": true,
        "use_variance_test": false
    },
    "graph": {
        "periodic": false,
        "sample_factor": 0.5,
        "overlap_factor_p2r": 1,
        "overlap_factor_r2p": 1,
        "regional_level": 1,
        "add_dummy_node": false,
        "with_additional_info": true,
        "regional_points": [128,128]
    },
    "model": {
        "name": "lano",
        "drop_edge": 0.0,
        "use_conditional_norm": false,
        "variable_mesh": false,
        "args": {
            "gno": {
                "gno_coord_dim": 2,
                "projection_channels": 256,
                "in_gno_channel_mlp_hidden_layers": [
                    64,
                    64,
                    64
                ],
                "out_gno_channel_mlp_hidden_layers": [
                    64,
                    64
                ],
                "lifting_channels": 32,
                "gno_radius": 0.033,
                "attention_type": "cosine",
                "use_geoembed": false,
                "embedding_method": "pointnet"
            },
            "transformer": {
                "patch_size": 8,
                "hidden_size": 256,
                "use_attn_norm": true,
                "use_ffn_norm": true,
                "norm_eps": 1e-06,
                "num_layers": 5,
                "positional_embedding": "rope",
                 "use_long_range_skip": true,
                "attn_config": {
                    "hidden_size": 256,
                    "num_heads": 8,
                    "num_kv_heads": 8,
                    "use_conditional_norm": false,
                    "cond_norm_hidden_size": 4,
                    "atten_dropout": 0.2
                },
                "ffn_config": {
                    "hidden_size": 1024,
                    "use_conditional_norm": false,
                    "cond_norm_hidden_size": 4
                }
            }
        }
    },
    "dataset": {
        "name": "airfoil_grid",
        "metaname": "rigno-unstructured/airfoil_grid",
        "base_path": "/cluster/work/math/camlab-data/rigno-unstructured/",
        "train_size": 2048,
        "val_size": 128,
        "test_size": 256,
        "batch_size": 64,
        "num_workers": 4,
        "shuffle": true
    },
    "optimizer": {
        "name": "adamw",
        "args": {
            "lr": 1e-4,
            "weight_decay": 1e-05,
            "epoch": 1000,
            "loss_scale": 1.0,
            "eval_every_eps": 2,
            "scheduler": "mix",
            "max_lr": 1e-4,
            "min_lr": 1e-5,
            "final_lr": 1e-5
        }
    },
    "path": {
        "ckpt_path": ".ckpt/gaot/static/airfoil_grid.pt",
        "loss_path": ".loss/gaot/static/airfoil_grid.png",
        "result_path": ".results/gaot/static/airfoil_grid.png",
        "database_path": ".database/gaot/static/airfoil_grid.csv"
    }
}