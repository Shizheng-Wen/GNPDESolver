{  
    "setup": {
        "seed": 42,
        "device": "cuda:0",
        "dtype": "torch.float32",
        "trainer_name": "static",
        "train": true,
        "test" : false,
        "ckpt": false,
        "use_variance_test": false
    },

    "graph": {
        "periodic": false,
        "sample_factor": 0.5,
        "overlap_factor_p2r": 1,
        "overlap_factor_r2p": 1,
        "regional_level": 1,
        "add_dummy_node": false,
        "with_additional_info": true,
        "regional_points": [64, 64]
    },

    "model": {
        "name": "rano",
        "drop_edge": 0.0,
        "use_conditional_norm": false,
        "variable_mesh": false,
        "args":{
            "deepgnn": {
                "node_latent_size": 16,
                "edge_latent_size": 16,
                "num_message_passing_steps": 3,
                "use_node_encode": true,
                "use_edge_encode": true,
                "use_node_decode": true,
                "use_edge_decode": true,
                "mpconfig": {
                    "aggregate": "mean",
                    "aggregate_normalization": null,
                    "use_node_fn": true,
                    "use_edge_fn": true,
                    "edge_fn_config": {
                        "hidden_size": 16,
                        "num_layers": 3,
                        "activation": "swish",
                        "use_layer_norm": true,
                        "use_conditional_norm": false,
                        "cond_norm_hidden_size": 4
                    },
                    "node_fn_config": {
                        "hidden_size": 16,
                        "num_layers": 3,
                        "activation": "swish",
                        "use_layer_norm": true,
                        "use_conditional_norm": false,
                        "cond_norm_hidden_size": 4
                    }
                }
            },
            "transformer": {
                "patch_size": 2,
                "hidden_size": 256,
                "use_attn_norm": true,
                "use_ffn_norm": true,
                "norm_eps": 1e-6,
                "num_layers": 5,
                "positional_embedding": "rope",
                "use_long_range_skip": true,
                "attn_config": {
                    "hidden_size": 256,
                    "num_heads": 8,
                    "num_kv_heads": 8,
                    "use_conditional_norm": false,
                    "cond_norm_hidden_size": 4
                },
                "ffn_config": {
                    "hidden_size": 256,
                    "use_conditional_norm": false,
                    "cond_norm_hidden_size": 4      
                }
            }
        }
    },

    "dataset": {
        "name": "elasticity",
        "metaname": "rigno-unstructured/elasticity",
        "base_path": "/cluster/work/math/camlab-data/rigno-unstructured/",
        "train_size": 1024,
        "val_size": 128,
        "test_size": 256,
        "batch_size": 64,
        "num_workers": 4,
        "shuffle": true,
        "metric": "final_step",
        "predict_mode": "autoregressive"
    },

    "optimizer": {
        "name": "adamw",
        "args": {
            "lr": 1e-3,
            "weight_decay": 1e-2,
            "epoch": 500,
            "loss_scale": 1.0,
            "eval_every_eps": 2,
            "scheduler": "mix",
            "max_lr": 5e-3,
            "min_lr": 1e-3,
            "final_lr": 5e-4
        }
    },

    "path": {
        "ckpt_path":".ckpt/rano/elasticity.pt",
        "loss_path":".loss/rano/elasticity.png",
        "result_path":".results/rano/elasticity.png",
        "database_path":".database/rano/elasticity.csv"
    }
}   
                
            
