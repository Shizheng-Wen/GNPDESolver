{  
    "setup": {
        "seed": 42,
        "device": "cuda:0",
        "dtype": "torch.float32",
        "trainer_name": "seq",
        "train": true,
        "test" : false,
        "ckpt": false,
        "use_variance_test": false
    },

    "graph": {
        "periodic": false,
        "sample_factor": 0.5,
        "overlap_factor_p2r": 1,
        "overlap_factor_r2p": 1,
        "regional_level": 1,
        "add_dummy_node": false,
        "with_additional_info": true,
        "regional_points": [64, 64]
    },

    "model": {
        "name": "gino",
        "drop_edge": 0.0,
        "variable_mesh": false,
        "args": {
            "gno_coord_dim": 2,
            "projection_channels": 256,
            "in_gno_channel_mlp_hidden_layers": [64, 64, 64],
            "out_gno_channel_mlp_hidden_layers": [64, 64],
            "fno_in_channels": 64,
            "fno_n_modes": [16, 16],
            "lifting_channels": 256,
            "fno_n_layers": 3,
            "in_gno_transform_type": "linear",
            "out_gno_transform_type": "linear",
            "gno_radius": 0.033
        }
    },

    "dataset": {
        "name": "heat_l_sines",
        "metaname": "rigno-unstructured/heat_l_sines",
        "base_path": "../graph_pde_solver/dataset",
        "train_size": 1024,
        "val_size": 128,
        "test_size": 256,
        "max_time_diff": 14,
        "batch_size": 64,
        "num_workers": 4,
        "shuffle": true,
        "latent_queries": [64, 64],
        "metric": "all_step",
        "predict_mode": "autoregressive"
    },

    "optimizer": {
        "name": "adam",
        "args": {
            "lr": 1e-3,
            "epoch": 6,
            "batch_size": 32,
            "loss_scale": 1.0,
            "eval_every_eps": 2,
            "scheduler": "step",
            "scheduler_step_size": 100,
            "scheduler_gamma": 0.8,
            "scheduler_T_max": 100,
            "scheduler_eta_min": 1e-4
        }
    },

    "path": {
        "ckpt_path":".ckpt/gino/test/heat_l_sines.pt",
        "loss_path":".loss/gino/test/heat_l_sines.png",
        "result_path":".results/gino/test/heat_l_sines.png",
        "database_path":".database/gino/test/heat_l_sines.csv"
    }

}


