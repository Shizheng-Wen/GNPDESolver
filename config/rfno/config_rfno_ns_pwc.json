{  
    "setup": {
        "seed": 42,
        "device": "cuda:0",
        "dtype": "torch.float32",
        "trainer_name": "sequential",
        "train": true,
        "test" : false,
        "ckpt": false,
        "use_variance_test": false
    },

    "graph": {
        "periodic": false,
        "sample_factor": 0.5,
        "overlap_factor_p2r": 1,
        "overlap_factor_r2p": 1,
        "regional_level": 1,
        "add_dummy_node": false,
        "with_additional_info": true,
        "regional_points": [64, 64]
    },

    "model": {
        "name": "rfno",
        "drop_edge": 0.0,
        "use_conditional_norm": false,
        "variable_mesh": false,
        "args":{
            "patch_size": 2,
            "deepgnn": {
                "node_latent_size": 16,
                "edge_latent_size": 16,
                "num_message_passing_steps": 3,
                "use_node_encode": true,
                "use_edge_encode": true,
                "use_node_decode": true,
                "use_edge_decode": true,
                "mpconfig": {
                    "aggregate": "mean",
                    "aggregate_normalization": null,
                    "use_node_fn": true,
                    "use_edge_fn": true,
                    "edge_fn_config": {
                        "hidden_size": 16,
                        "num_layers": 3,
                        "activation": "swish",
                        "use_layer_norm": true,
                        "use_conditional_norm": false,
                        "cond_norm_hidden_size": 4
                    },
                    "node_fn_config": {
                        "hidden_size": 16,
                        "num_layers": 3,
                        "activation": "swish",
                        "use_layer_norm": true,
                        "use_conditional_norm": false,
                        "cond_norm_hidden_size": 4
                    }
                }
            },
            "fno": {
                "n_layers": 3,
                "n_modes": [16,16],
                "hidden_channels": 64,
                "lifting_channels": 256
            }
        }
    },

    "dataset": {
        "name": "NS-PwC",
        "metaname": "rigno-unstructured/NS-PwC",
        "base_path": "/cluster/work/math/camlab-data/rigno-unstructured/",
        "train_size": 1024,
        "val_size": 128,
        "test_size": 256,
        "max_time_diff": 14,
        "batch_size": 64,
        "num_workers": 4,
        "shuffle": true,
        "latent_queries": [64, 64],
        "metric": "final_step",
        "predict_mode": "autoregressive"
    },

    "optimizer": {
        "name": "adam",
        "args": {
            "lr": 1e-3,
            "epoch": 500,
            "batch_size": 32,
            "loss_scale": 1.0,
            "eval_every_eps": 2,
            "scheduler": "step",
            "scheduler_step_size": 100,
            "scheduler_gamma": 0.8,
            "scheduler_T_max": 100,
            "scheduler_eta_min": 1e-4
        }
    },


    "path": {
        "ckpt_path":".ckpt/rfno/ns_pwc.pt",
        "loss_path":".loss/rfno/ns_pwc.png",
        "result_path":".results/rfno/ns_pwc.png",
        "database_path":".database/rfno/ns_pwc.csv"
    }
}   
                
            
