{  
    "setup": {
        "seed": 42,
        "device": "cuda:0",
        "dtype": "torch.FloatTensor",
        "trainer_name": "static",
        "train": true,
        "test" : false,
        "ckpt": false,
        "use_variance_test": false
    },

    "model": {
        "name": "gino",
        "args": {
            "gno_coord_dim": 2,
            "projection_channels": 256,
            "in_gno_channel_mlp_hidden_layers": [64, 64, 64],
            "out_gno_channel_mlp_hidden_layers": [64, 64],
            "fno_in_channels": 64,
            "fno_n_modes": [16, 16],
            "lifting_channels": 256,
            "fno_n_layers": 3
        }
    },

    "dataset": {
        "name": "airfoil_li",
        "metaname": "rigno-unstructured/airfoil_li",
        "base_path": "/cluster/work/math/camlab-data/rigno-unstructured/",
        "train_size": 2048,
        "val_size": 128,
        "test_size": 256,
        "batch_size": 64,
        "num_workers": 4,
        "shuffle": true,
        "latent_queries": [96, 96]
    },

    "optimizer": {
        "name": "adam",
        "args": {
            "lr": 1e-3,
            "epoch": 1000,
            "batch_size": 32,
            "loss_scale": 1.0,
            "eval_every_eps": 2,
            "scheduler": "step",
            "scheduler_step_size": 100,
            "scheduler_gamma": 0.8,
            "scheduler_T_max": 200,
            "scheduler_eta_min": 1e-4
        }
    },

    "path": {
        "ckpt_path":".ckpt/gino/airfoil_li_ml.pt",
        "loss_path":".loss/gino/airfoil_li_ml.png",
        "result_path":".results/gino/airfoil_li_ml.png",
        "database_path":".database/gino/airfoil_li_ml.csv"
    }

    
}


